{
  "name": "node-grpc-typescript-labo",
  "version": "0.1.0",
  "description": "Node.js gRPC Structure for TypeScript Example",
  "main": "dist/server.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts .",
    "build": "rimraf dist && tsc -b",
